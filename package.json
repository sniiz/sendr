{
  "name": "sendr",
  "version": "1.0.0",
  "description": "a minimalist messaging app",
  "author": "sniiz",
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "preview": "node scripts/build.js",
    "deploy": "node scripts/build.js --production",
    "reinstall": "rm -rf node_modules && yarn install",
    "open": "open https://sendr-sniiz.vercel.app/",
    "pull": "git pull -X theirs && yarn reinstall",
    "g": "open https://github.com/sniiz/sendr"
  },
  "build": {
    "appId": "com.sniiz.sendr"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@shopify/flash-list": "^1.2.1",
    "expo": "~44.0.0",
    "expo-clipboard": "~2.1.0",
    "expo-image-picker": "~12.0.1",
    "expo-linking": "~3.0.0",
    "expo-localization": "~12.0.0",
    "firebase": "9.6.7",
    "node-notifier": "^10.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-elements": "^3.4.2",
    "react-native-feather": "^1.1.2",
    "react-native-popable": "^0.4.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
