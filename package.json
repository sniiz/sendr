  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "~0.16.2",
    "@types/react": "~17.0.21",
    "electron": "^18.0.1",
    "electron-builder": "^22.14.13",
    "typescript": "~4.3.5"
  }
